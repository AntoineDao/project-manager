{
  "name": "project-manager",
  "version": "1.0.0",
  "description": "A Github App that helps manage issues, pull requests and on Github project boards",
  "author": "Antoine Dao",
  "license": "ISC",
  "repository": "https://github.com//project-manager.git",
  "homepage": "https://github.com//project-manager",
  "bugs": "https://github.com//project-manager/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "probot": "^7.2.0",
    "probot-config": "^1.1.0"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}

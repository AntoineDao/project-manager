{
  "name": "@antoinedao/project-manager",
  "version": "0.0.0",
  "description": "A Github App that helps manage issues, pull requests and on Github project boards",
  "author": "Antoine Dao",
  "license": "ISC",
  "repository": "https://github.com/antoinedao/project-manager.git",
  "homepage": "https://github.com/antoinedao/project-manager",
  "bugs": "https://github.com/antoinedao/project-manager/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./lib/index.js",
    "lint": "standard --fix",
    "test": "echo no tests supplied",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "main": "./lib/index.js",
  "files": [
    "/lib"
  ],
  "dependencies": {
    "@probot/serverless-lambda": "^0.3.0",
    "lodash": "^4.17.15",
    "probot": "^7.2.0",
    "probot-config": "^1.1.0"
  },
  "devDependencies": {
    "@semantic-release/exec": "^3.3.6",
    "cz-conventional-changelog": "^3.0.2",
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "semantic-release": "^17.2.3",
    "serverless": "^1.52.0",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

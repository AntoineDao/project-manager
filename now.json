{
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "env": {
    "APP_ID": "@app-id",
    "PRIVATE_KEY": "@private-key-base64-encoded",
    "WEBHOOK_SECRET": "@webhook-secret"
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.js"
    }
  ],
  "version": 2
}